<!-- app/views/user_list/index.html.erb -->
<h2>Customers</h2>
<% if @customers.any? %>
  <div class="table-responsive">
    <table class="table table-striped table-hover">
      <!-- Table headers -->
        <thead>
        <tr>
          <th>User ID</th>
          <th>UserName</th>
          <th>Email</th>
          <th>Role</th>
          <!-- Add more table headers for other user attributes -->
        </tr>
      </thead>
      <tbody>
        <% @customers.each do |user| %>
          <tr>
            <!-- Table cells for customer attributes -->
            <tr>
            <td><%= user.id %></td>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.role %></td>

            <!-- Add more table cells for other user attributes -->
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<% else %>
  <p>No Customers Have Signed Up Yet.</p>
<% end %>

